{
  "name": "@philihp/score",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "node build-site.js && harp compile src public",
    "test": "jest && npm run lint",
    "lint": "eslint . --ext js,json",
    "server": "node build-site.js && harp server src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philihp/score.git"
  },
  "author": "Philihp Busby <philihp@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/philihp/score/issues"
  },
  "homepage": "https://github.com/philihp/score#readme",
  "dependencies": {
    "csv-parser": "3.0.0",
    "fast-shuffle": "4.2.2",
    "glob": "7.1.6",
    "harp": "0.33.0",
    "openskill": "1.0.1",
    "ramda": "0.27.1"
  },
  "devDependencies": {
    "@philihp/eslint-config": "4.1.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.2",
    "prettier": "2.2.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix"
    ],
    "**/*.{md,json,css,scss}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "@philihp"
    ],
    "rules": {
      "unicorn/filename-case": 0
    }
  }
}
